#include<stdio.h>
int main()
{
	int number;
	char ch[]={"该数是偶数"};
	scanf("%d",&number);
	if(number%2==1)
	{
		printf("该数是奇数");
	}
	else
	{
		printf("%s",ch);
	}
	int number1=1;
	while(number1>=0&&number1<=99)
	{
        if(number1%2==1)
		{
			printf("%d ",number1);
			number1+=1;
		}
		else
		{
			number1+=1;
		}
	}
	return 0;
}
