int addStrings(char * num1, char * num2)
{
	int e1=strlen(num1);
	int e2=strlen(num2);
	int i,j;
	i=0;
	int ii=1;
	j=0;
	int jj=1;
	int sum1=0;
	int sum2=0;
	while(i<e1&&ii<=e1)
	{
		sum1+=(num1[i]-48)*pow(10,e1-ii);
		i++;
		ii++;
	}
	while(j<e2&&jj<=e2)
	{
		sum2+=(num2[j]-48)*pow(10,e2-jj);
		j++;
		jj++;
	}
	return sum1+sum2;
}
